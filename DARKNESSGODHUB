    aimEnabled = not aimEnabled
    aimToggleBtn.Text = aimEnabled and "Aimlock: ON" or "Aimlock: OFF"
    aimToggleBtn.BackgroundColor3 = aimEnabled and Color3.fromRGB(0, 255, 180) or Color3.fromRGB(0, 180, 255)
end)

