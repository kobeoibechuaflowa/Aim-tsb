-- OrionLib fix cho KRNL
local success, lib = pcall(function()
    return loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()
end)

if not success then
    warn("KhÃ´ng táº£i Ä‘Æ°á»£c OrionLib, thá»­ láº¡i sau")
    return
end

local OrionLib = lib

local Window = OrionLib:MakeWindow({Name = "TSB HUB", HidePremium = false, SaveConfig = false})

local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local FramKill = false
local AimBot = false

-- Fram Kill
MainTab:AddToggle({
    Name = "Fram Kill",
    Default = false,
    Callback = function(Value)
        FramKill = Value
        task.spawn(function()
            while FramKill do
                task.wait(0.3)
                for _,v in pairs(game.Players:GetPlayers()) do
                    if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                        pcall(function()
                            game:GetService("ReplicatedStorage").Remotes.Attack:FireServer(v.Character)
                        end)
                    end
                end
            end
        end)
    end    
})

-- AimBot
MainTab:AddToggle({
    Name = "Aim Lock",
    Default = false,
    Callback = function(Value)
        AimBot = Value
    end    
})

game:GetService("RunService").RenderStepped:Connect(function()
    if AimBot and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local closestPlayer, shortestDistance = nil, math.huge
        for _,v in pairs(game.Players:GetPlayers()) do
            if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                local dist = (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude
                if dist < shortestDistance then
                    shortestDistance = dist
                    closestPlayer = v
                end
            end
        end
        if closestPlayer then
            workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, closestPlayer.Character.HumanoidRootPart.Position)
        end
    end
end)

-- Táº¯t UI
MainTab:AddButton({
    Name = "ÄÃ³ng UI",
    Callback = function()
        OrionLib:Destroy()
    end    
})

OrionLib:Init()
        task.wait(0.3)
        HubFrame.Visible = false
    end
end)

